--     | (?1#(%1 <= 3), ?1#(%1 >= 4), ?1#(%1 >= 4), ?1#(%1 >= 4)) -- R_α_c_a_b (= - R_a_b_c_α) 1456
--       -> let (α, c, a, b) := (i, j - 3, k - 3, l - 3)
--            in -1 * (-1 * p_α * ∇A_a_α_b_c + p_α * ∇A_b_α_a_c)
--     | (?1#(%1 >= 4), ?1#(%1 >= 4), ?1#(%1 <= 3), ?1#(%1 >= 4)) -- R_a_b_α_c (= - R_a_b_c_α) 4516
--       -> let (a, b, α, c) := (i - 3, j - 3, k, l - 3)
--            in -1 * (-1 * p_α * ∇A_a_α_b_c + p_α * ∇A_b_α_a_c)
     | (?1#(%1 >= 4), ?1#(%1 >= 4), ?1#(%1 <= 3), ?1#(%1 <= 3)) -- R_a_b_α_β
       -> let (a, b, α, β) := (i - 3, j - 3, k, l)
            in p_α * p_β * (-1 * g_c_d . A_β_a~c . A_α_b~d + g_c_d . A_β_b~c . A_α_a~d)
     | (?1#(%1 <= 3), ?1#(%1 <= 3), ?1#(%1 >= 4), ?1#(%1 >= 4)) -- R_α_β_a_b (= R_a_b_α_β)
       -> let (α, β, a, b) := (i, j, k - 3, l - 3)
            in p_α * p_β * (-1 * g_c_d . A_β_a~c . A_α_b~d + g_c_d . A_β_b~c . A_α_a~d)
     | (?1#(%1 >= 4), ?1#(%1 <= 3), ?1#(%1 <= 3), ?1#(%1 >= 4)) -- R_a_α_β_b
       -> let (a, α, β, b) := (i - 3, j, k, l - 3)
            in withSymbols [c, d] p_α * p_β * g_c_d . A_β_a~c . A_α_b~d - p_β * g_c_b . A_β_a~c * s_α_β
     | (?1#(%1 <= 3), ?1#(%1 >= 4), ?1#(%1 <= 3), ?1#(%1 >= 4)) -- R_α_a_β_b (= -1 R_a_α_β_b)
       -> let (α, a, β, b) := (i, j - 3, k, l - 3)
            in withSymbols [c, d] -1 * p_α * p_β * g_c_d . A_β_a~c . A_α_b~d - p_β * g_c_b . A_β_a~c * s_α_β
     | (?1#(%1 <= 3), ?1#(%1 >= 4), ?1#(%1 >= 4), ?1#(%1 <= 3)) -- R_β_b_a_α (= R_a_α_β_b)
       -> let (β, b, a, α) := (i, j - 3, k - 3, l)
            in withSymbols [c, d] p_α * p_β * g_c_d . A_β_a~c . A_α_b~d - p_β * g_c_b . A_β_a~c * s_α_β
     | (?1#(%1 >= 4), ?1#(%1 <= 3), ?1#(%1 >= 4), ?1#(%1 <= 3)) -- R_a_α_b_β (= -1 R_a_α_β_b)
