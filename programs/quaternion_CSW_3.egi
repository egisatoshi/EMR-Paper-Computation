R'_i_j_k_l := (generateTensor
  (\i j k l -> match (i, j, k, l) as (integer, integer, integer, integer) with
     | (?1#(%1 >= 4), ?1#(%1 >= 4), ?1#(%1 >= 4), ?1#(%1 >= 4))
